{
  "name": "Finteo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "start": "react-native start",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "prepare": "husky",
    "e2e:ios:build": "xcrun xcodebuild -scheme 'BetterMe' -workspace './ios/BetterMe.xcworkspace' -configuration Release -sdk 'iphonesimulator' -destination 'generic/platform=iOS Simulator' -derivedDataPath ios/build",
    "e2e:android:build": "cd android && ./gradlew assembleRelease",
    "e2e:android:install": "adb install emulator-5554 android/app/build/outputs/apk/release/app-release.apk",
    "e2e:ios:install": "xcrun simctl install 2D0B4DF2-7D80-45BC-923B-9D98D4855ACC ./ios/build/Build/Products/Release-iphonesimulator/Finteo.app",
    "e2e:ios:run": "maestro --device 2D0B4DF2-7D80-45BC-923B-9D98D4855ACC test e2e/* ",
    "e2e:android:run": "maestro --device emulator-5554 test e2e/*"
  },
  "dependencies": {
    "@hookform/resolvers": "4.1.3",
    "@lodev09/react-native-true-sheet": "2.0.5",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-native-firebase/analytics": "21.7.1",
    "@react-native-firebase/app": "21.7.1",
    "@react-native-firebase/auth": "21.7.1",
    "@react-native-firebase/crashlytics": "21.7.1",
    "@react-native-firebase/firestore": "21.7.1",
    "@react-native-firebase/remote-config": "21.7.1",
    "@react-navigation/bottom-tabs": "7.2.0",
    "@react-navigation/native": "7.0.14",
    "@react-navigation/native-stack": "7.2.0",
    "@tanstack/react-query": "5.66.11",
    "date-fns": "4.1.0",
    "eslint-plugin-ft-flow": "3.0.11",
    "i18next": "24.2.3",
    "react": "19.0.0",
    "react-hook-form": "7.54.2",
    "react-i18next": "15.5.2",
    "react-native": "0.79.0",
    "react-native-date-picker": "5.0.12",
    "react-native-edge-to-edge": "1.4.3",
    "react-native-email-link": "1.16.1",
    "react-native-linear-gradient": "2.8.3",
    "react-native-localize": "3.4.1",
    "react-native-nitro-modules": "0.24.1",
    "react-native-progress": "5.0.1",
    "react-native-safe-area-context": "5.4.1",
    "react-native-screens": "4.10.0",
    "react-native-toast-message": "2.3.0",
    "react-native-unistyles": "3.0.0-rc.4",
    "react-native-uuid": "2.0.3",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.3",
    "@babel/runtime": "7.25.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "8.57.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.0",
    "@react-native/eslint-config": "0.79.0",
    "@react-native/metro-config": "0.79.0",
    "@react-native/typescript-config": "0.79.0",
    "@tanstack/eslint-plugin-query": "5.66.1",
    "@testing-library/react-native": "13.0.1",
    "@types/jest": "29.5.13",
    "@types/react": "19.0.0",
    "@types/react-test-renderer": "19.0.0",
    "eslint": "8.57.0",
    "eslint-plugin-testing-library": "7.1.1",
    "husky": "9.1.7",
    "jest": "29.6.3",
    "lint-staged": "15.4.3",
    "prettier": "3.5.0",
    "react-test-renderer": "19.0.0",
    "reassure": "1.4.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
